{
    "env": {
        "browser": false,
        "node": true
        // "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": "off",
        "max-len": "warn",
        "require-jsdoc": "off"
    }
}